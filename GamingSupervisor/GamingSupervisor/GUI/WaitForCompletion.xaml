<Page x:Class="GamingSupervisor.WaitForCompletion"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GamingSupervisor"
      mc:Ignorable="d" 
      d:DesignHeight="590" d:DesignWidth="480"
      Title="WaitForCompletion">
    <DockPanel Width="480" HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock TextAlignment="Center" TextWrapping="Wrap" Name="ParsingMessageLabel" FontSize="16" Foreground="{StaticResource Foreground}"></TextBlock>
    </DockPanel>
</Page>
